{
  "name": "shijimane-shino.github.io",
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "lint": "run-p --continue-on-error lint:*",
    "lint:prettier": "prettier --loglevel warn --check --ignore-path .gitignore .",
    "lint:stylelint": "stylelint \"**/*.css\" \"**/*.scss\" --ignore-path .gitignore",
    "lint:eslint": "eslint --ext \".js,.jsx,.ts,.tsx\" --ignore-path .gitignore .",
    "fix": "run-p --continue-on-error fix:*",
    "fix:prettier": "prettier --loglevel warn --write --ignore-path .gitignore .",
    "fix:stylelint": "stylelint \"**/*.css\" \"**/*.scss\" --fix --ignore-path .gitignore",
    "fix:eslint": "eslint --ext \".js,.jsx,.ts,.tsx\" --fix --ignore-path .gitignore .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shijimane-shino/shijimane-shino.github.io.git"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0-beta3",
    "@fortawesome/free-regular-svg-icons": "^6.0.0-beta3",
    "@fortawesome/free-solid-svg-icons": "^6.0.0-beta3",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ress": "^4.0.0",
    "sass": "^1.45.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addons": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@storybook/theming": "^6.4.9",
    "@types/node": "17.0.4",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "4.5.4"
  }
}
