{
  "name": "shijimane-shino.github.io",
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "lint": "run-p --continue-on-error lint:*",
    "lint:prettier": "prettier --loglevel warn --check .",
    "lint:stylelint": "stylelint \"*/.css\" \"*/.scss\"",
    "lint:eslint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "fix": "run-p --continue-on-error fix:*",
    "fix:prettier": "prettier --loglevel warn --write .",
    "fix:stylelint": "stylelint \"*/.css\" \"*/.scss\" --fix",
    "fix:eslint": "eslint --ext \".js,.jsx,.ts,.tsx\" --fix ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shijimane-shino/shijimane-shino.github.io.git"
  },
  "dependencies": {
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.45.1"
  },
  "devDependencies": {
    "@types/node": "17.0.4",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "4.5.4"
  }
}
